package com.yourorg.telemedicine.ai.client;

import org.springframework.cloud.openfeign.FeignClient;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PathVariable;

import com.yourorg.telemedicine.ai.dto.PatientDTO;



@FeignClient(name="patient-service", url="${patient.service.url:http://localhost:8083}")
public interface PatientClient {
  @GetMapping("/patients/{id}")
  PatientDTO getPatient(@PathVariable("id") Long id);


}
